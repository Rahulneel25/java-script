    <script>
        let a = parseInt(prompt("working"))
        let b = parseInt(prompt("absent"))
        let e = a-b
        let c = e/a
        let att = c*100
        if(att<75){
            alert("not eligible")
        } else {
            document.write("eligible")
            alert("elgible")
        }
        document.write("welcome")

    </script>
    
