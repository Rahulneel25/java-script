    <script>
        let a = parseInt(prompt("give a number"))
        if(a%2==0||a%3==0){
            alert("divisible")
        }else if(a%2==0&&a%3==0){
            alert("divided")
        }else{
            alert("not divided")
        }
        document.write("welcome")

    </script>
    
</body>
