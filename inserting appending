    <div id="a">
        <h1>ravi</h1>
        <h1 id = "r">ram</h1>
    </div>
    <div id="b">
        <h2 id = "n">raju</h2>
        <h2 >raghu</h2>
    </div>

    <script>
        let n = document.createElement("h1")
        n.textContent="version-it"
        let r = document.createElement("h2")
        r.textContent="Rahul"
        let e = document.getElementById("n")
        let m = document.getElementById("a")
        let l = document.getElementById("r")
        //replace all children with version it
        m.replaceChildren(n)
        e.replaceChild(r)
        console.log(e)
        
        console.log(m)

    </script>
</body>
</html>
