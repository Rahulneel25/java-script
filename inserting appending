<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <div id = "a">
        <h1 id = "n">Rahul</h1>
        <h1 id = "r">Revanth</h1>
    </div>
    <div id = "b">
        <h1>Sathvik</h1>
        <h1>Abhiram</h1>

    </div>
  
    <script>
        let n = document.createElement("h2")
        n.textContent = "Rahulneel"
        //inserting rahulneel
        let k = document.createElement("h2")
        k.textContent = "Rafael"
        //inserting rafael
     
        let p = document.getElementById("a")
        let l = document.getElementById("r")
        let m = document.getElementById("n")
        
        p.insertBefore(k,m)
        p.insertBefore(n,l)
        console.log(p)
       
       
    </script>
</body>
</html>
